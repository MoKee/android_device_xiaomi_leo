service displayfeature /system/bin/displayfeature
    class late_start
    user root
    group root

on property:persist.sys.aries.power_profile=high
    stop thermal-engine
    write /sys/devices/system/cpu/cpu4/core_ctl/max_cpus 4
    write /sys/devices/system/cpu/cpu4/core_ctl/min_cpus 4
 
on property:persist.sys.aries.power_profile=middle
    stop thermal-engine
    write /sys/devices/system/cpu/cpu4/core_ctl/max_cpus 4
    write /sys/devices/system/cpu/cpu4/core_ctl/min_cpus 2
    start thermal-engine
 
on property:persist.sys.aries.power_profile=low
    stop thermal-engine
    write /sys/devices/system/cpu/cpu4/core_ctl/max_cpus 0
    start thermal-engine

